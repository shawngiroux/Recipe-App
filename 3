import React from 'react';

class Recipe extends React.Component {
    constructor(props) {
        super(props);
        fetch('localhost/recipe/1')
            .then(response => response.json())
            .then(
                data => {
                    console.log(data);
                }
            )
    }
    render() {
        return (
            <div>
            </div>
        );
    }
}
